{
	"method" : "PUT",
	"url" : "{hyperdata_url}/hyperdata/hd-auth/User",
	"header": {
		"Content-Type" : "application/json",
		"Authorization" : "{token}",
		"userId" : "{user_id}"
	},
	"data_raw": {
		"dto" : {
			"userId" : "{hyperdata_user_id_enc}" ,
			"passwd" : "{hyperdata_user_pw_enc}" ,
			"userName" : "{hyperdata_user_name_enc}" ,
			"userEmail" : "{hyperdata_user_email_enc}" ,
			"userPhoneNumber" : "{hyperdata_user_phone_enc}"
		}
	}
}

